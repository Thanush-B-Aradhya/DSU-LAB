    "n = int(input(\"Enter the number: \"))\n",
    "sum = 0\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    if(i%2 == 0):\n",
    "        term = i*i\n",
    "    else:\n",
    "        term = 0\n",
    "    sum = sum + term\n",
    "print(\"Sum of square of even numbers less than\" ,n , \"is\", sum)"
